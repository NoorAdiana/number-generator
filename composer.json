{
    "name": "inisiatif/number-generator",
    "description": "For internal purposes to create a unique string of number sequences",
    "keywords": ["laravel", "illuminate", "generator"],
    "authors": [
        {
            "name": "Nuradiyana",
            "email": "me@nooradiana.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "laravel/framework": "5.4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\NumberGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
        "laravel": {
            "providers": [
                "Inisiatif\\NumberGenerator\\NumberGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
